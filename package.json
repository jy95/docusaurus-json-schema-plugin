{
  "name": "docusaurus-json-schema-plugin",
  "version": "0.0.0-development",
  "description": "A Docusaurus theme plugin that provides a JSON Schema viewer / editor React component !",
  "keywords": [
    "docusaurus",
    "docusaurus theme",
    "docusaurus plugin",
    "json schema",
    "json schema viewer",
    "json schema documentation",
    "json schema editor"
  ],
  "main": "lib/index.js",
  "types": "src/theme-json-schema.d.ts",
  "repository": "https://github.com/jy95/docusaurus-json-schema-plugin.git",
  "homepage": "https://jy95.github.io/docusaurus-json-schema-plugin/",
  "author": "jy95",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "tsc && node copyUntypedFiles.mjs",
    "prettier": "prettier --write **/*.{js,jsx,ts,tsx,json}",
    "semantic-release": "semantic-release",
    "test": "npx jest",
    "test:coverage": "npx jest --coverage"
  },
  "prettier": {
    "semi": false
  },
  "exports": {
    "./lib/*": "./lib/*",
    "./src/*": "./src/*",
    ".": {
      "types": "./src/theme-json-schema.d.ts",
      "default": "./lib/index.js"
    }
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.7.0",
    "@docusaurus/types": "^3.7.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.2",
    "@types/react": "^19.0.8",
    "fs-extra": "^11.1.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-schema-typed": "^8.0.1",
    "prettier": "^3.0.1",
    "semantic-release": "^24.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "@docusaurus/core": "^3.7.0",
    "@docusaurus/theme-classic": "^3.7.0",
    "react": ">=18 <= 19"
  },
  "dependencies": {
    "@stoplight/json-ref-resolver": "^3.1.5",
    "monaco-editor": "^0.52.2",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "react-monaco-editor": "^0.58.0"
  }
}
